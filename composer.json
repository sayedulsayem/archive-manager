{
	"name": "sayedul-sayem/archive-manager",
	"description": "Easily access your archive pages for taxonomies and users",
	"type": "wordpress-plugin",
	"scripts": {
		"build": [
			"@composer --no-plugins --no-interaction install --no-scripts --no-dev"
		]
	},
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.2",
		"squizlabs/php_codesniffer": "^3.13"
	},
	"license": "GPL-2.0-or-later",
	"autoload": {
		"psr-4": {
			"SayedulSayem\\ArchiveManager\\": "includes/"
		}
	},
	"authors": [
		{
			"name": "Sayedul Sayem",
			"email": "sayedulsayem@gmail.com"
		}
	],
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}